{
    "name": "farzai/kapi",
    "description": "This SDK simplifies the process of integrating with KBank's APIs",
    "keywords": [
        "farzai",
        "kapi",
        "kbank",
        "kbank api",
        "kbank api sdk",
        "open api",
        "sdk"
    ],
    "homepage": "https://github.com/farzai/kapi",
    "license": "MIT",
    "authors": [
        {
            "name": "parsilver",
            "email": "parkorn@farzai.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.5",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\KApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\KApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
